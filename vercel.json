{
  "name": "api-temp",
  "version": 2,
  "builds": [
    {
      "src": "handler.py",
      "use": "@now/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/handler.py"
    }
  ],
  "functions": {
    "notificar_temperatura": {
      "schedule": "*/30 * * * *",
      "maxDuration": 10,
      "runtime": "@now/python"
    }
  }
}